{
  "name": "project-q",
  "version": "1.0.0",
  "description": "",
  "main": "index.tsx",
  "scripts": {
    "build:dev": "webpack --config ./webpack/dev.config.js",
    "build:prod": "webpack --config ./webpack/prod.config.js",
    "dev-client": "webpack-dev-server --config ./webpack/dev.config.js --hot",
    "dev-server": "webpack --config ./webpack/dev.config.js && NODE_ENV=development nodemon",
    "start": "NODE_ENV=production ts-node ./src/server/server.ts",
    "server": "nodemon",
    "daemon": "nodemon --exec ts-node ./src/server/daemonServer.ts",
    "test": "NODE_ENV=test jest",
    "test:coverage": "NODE_ENV=test webpack --config ./webpack/prod.config.js && jest --coverage",
    "test:watch": "NODE_ENV=test jest --watch",
    "test-updateSnapshot": "NODE_ENV=development jest --updateSnapshot",
    "add-dep": "bash ./scripts/npm-docker/add-dep.sh",
    "update-dep": "bash ./scripts/npm-docker/update-dep.sh",
    "image-remove": "docker image rm dbsites/ethiq-dependencies --force",
    "image-build": "docker build -t dbsites/ethiq-dependencies -f Dockerfile-dev .",
    "docker-remove-containers": "docker rm $(docker ps -a -q -f 'name=ethiq') --force",
    "docker-remove-images": "docker rmi $(docker images dbsites/ethiq* -q) --force",
    "docker-remove-volumes": "docker volume rm $(docker volume ls -q -f name=ethiq) --force",
    "docker-build:dev": "docker-compose run --rm --service-ports build-dev",
    "docker-dev": "docker-compose -f docker-comp-dev.yml up",
    "docker-test": "docker-compose -f docker-comp-test.yml up --abort-on-container-exit",
    "docker-dev:hot": "docker-compose -f docker-comp-dev-hot.yml up",
    "docker-stop": "docker stop $(docker ps -a -q --filter status=running)",
    "docker-remove-all:danger": "docker rm $(docker ps -a -q -f 'name=ethiq') --force && docker rmi $(docker images dbsites/*ethiq* -q) --force && docker volume rm $(docker volume ls -q -f name=project-q) --force && exit"
  },
